{
  "env": {
    "INDY_NETWORK": "HOST_DOCKER_INTERNAL",
    "ES_URL": "http://localhost:9200"
  },
  "workersBuilderTemplates": [
    {
      "builder": "rtwSerialization",
      "params": {
        "operationType": "ledgercopy",
        "indyNetworkId": "{{{INDY_NETWORK}}}",
        "genesisPath": "{{{cfgdir}}}/genesis/{{{INDY_NETWORK}}}.txn`",
        "esUrl": "{{{ES_URL}}}",
        "esIndex": "txs-localdocker",
        "workerTiming": "MEDIUM"
      }
    },
    {
      "builder": "rtwExpansion",
      "params": {
        "operationType": "dbexpansion",
        "indyNetworkId": "{{{INDY_NETWORK}}}",
        "esUrl": "{{{ES_URL}}}",
        "esIndex": "txs-localdocker",
        "workerTiming": "MEDIUM"
      }
    }
  ]
}


